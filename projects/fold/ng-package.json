{
	"$schema": "../../node_modules/ng-packagr/ng-package.schema.json",
	"dest": "../../dist/@fold",
	"inlineStyleLanguage": "scss",
	"deleteDestPath": true,
	"assets": [
		{ "input": "src/scss", "glob": "**/*.scss", "output": "scss" },
		{ "input": "src/fonts", "glob": "**/*.ttf", "output": "fonts" },
		{ "input": "src/images", "glob": "**/*.(png|jpg)", "output": "images" }
	],
	"lib": {
		"entryFile": "src/public-api.ts"
	}
}
