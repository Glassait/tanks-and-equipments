{
	"$schema": "./node_modules/@angular/cli/lib/config/schema.json",
	"version": 1,
	"newProjectRoot": "projects",
	"projects": {
		"tanks-and-equipments": {
			"projectType": "application",
			"schematics": {},
			"root": "",
			"sourceRoot": "src",
			"prefix": "",
			"architect": {
				"build": {
					"builder": "@angular-devkit/build-angular:application",
					"options": {
						"inlineStyleLanguage": "scss",
						"outputPath": "dist/tanks-and-equipments",
						"index": "src/index.html",
						"browser": "src/main.ts",
						"polyfills": ["zone.js"],
						"tsConfig": "tsconfig.app.json",
						"assets": [
							"src/favicon.ico",
							"src/assets",
							{
								"glob": "**/*",
								"input": "node_modules/fold/images",
								"output": "/images/"
							}
						],
						"styles": ["src/styles.scss"],
						"scripts": [],
						"server": "src/main.server.ts",
						"prerender": true,
						"ssr": {
							"entry": "server.ts"
						},
						"stylePreprocessorOptions": {
							"includePaths": ["node_modules/fold/scss/"]
						}
					},
					"configurations": {
						"production": {
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "500kb",
									"maximumError": "1mb"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "2kb",
									"maximumError": "4kb"
								}
							],
							"outputHashing": "all"
						},
						"external-mock": {
							"optimization": false,
							"extractLicenses": false,
							"sourceMap": true,
							"fileReplacements": [
								{
									"replace": "src/environments/environment.ts",
									"with": "src/environments/environment.external-mock.ts"
								}
							]
						},
						"full-mock": {
							"optimization": false,
							"extractLicenses": false,
							"sourceMap": true,
							"fileReplacements": [
								{
									"replace": "src/environments/environment.ts",
									"with": "src/environments/environment.full-mock.ts"
								}
							]
						}
					},
					"defaultConfiguration": "production"
				},
				"serve": {
					"builder": "@angular-devkit/build-angular:dev-server",
					"configurations": {
						"production": {
							"buildTarget": "tanks-and-equipments:build:production"
						},
						"external-mock": {
							"buildTarget": "tanks-and-equipments:build:external-mock"
						},
						"full-mock": {
							"buildTarget": "tanks-and-equipments:build:full-mock"
						}
					},
					"defaultConfiguration": "development"
				},
				"extract-i18n": {
					"builder": "@angular-devkit/build-angular:extract-i18n",
					"options": {
						"buildTarget": "tanks-and-equipments:build"
					}
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"codeCoverage": true,
						"karmaConfig": "karma.config.js",
						"polyfills": ["zone.js", "zone.js/testing"],
						"tsConfig": "tsconfig.spec.json",
						"assets": [],
						"styles": [],
						"scripts": []
					}
				}
			}
		},
		"fold": {
			"projectType": "library",
			"root": "projects/fold",
			"sourceRoot": "projects/fold/src",
			"prefix": "",
			"architect": {
				"build": {
					"builder": "@angular-devkit/build-angular:ng-packagr",
					"options": {
						"project": "projects/fold/ng-package.json"
					},
					"configurations": {
						"production": {
							"tsConfig": "projects/fold/tsconfig.lib.prod.json"
						},
						"development": {
							"tsConfig": "projects/fold/tsconfig.lib.json"
						}
					},
					"defaultConfiguration": "production"
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"codeCoverage": true,
						"karmaConfig": "karma.config.js",
						"polyfills": ["zone.js", "zone.js/testing"],
						"tsConfig": "projects/fold/tsconfig.spec.json",
						"assets": [],
						"styles": ["projects/fold/src/scss/main.scss"],
						"scripts": []
					}
				}
			}
		}
	},
	"cli": {
		"schematicCollections": ["@angular-eslint/schematics"]
	}
}
