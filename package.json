{
	"name": "tanks-and-equipments",
	"version": "3.0.2",
	"private": true,
	"engines": {
		"node": "18.18.0 || 18.20.4",
		"npm": "9.6.7 || 10.7.0"
	},
	"scripts": {
		"start": "npm run serve:ssr:tanks-and-equipments",
		"build": "ng build --configuration production",
		"serve:ssr:tanks-and-equipments": "node dist/tanks-and-equipments/server/server.mjs",
		"start:local": "ng serve --port=4200",
		"start:external-mock": "npm run start:local -- --configuration=external-mock",
		"start:full-mock": "npm run start:local -- --configuration=full-mock",
		"test": "ng test --code-coverage --watch=false",
		"test:ci": "npm run test -- --browsers=ChromeHeadless",
		"test:local": "npm run test -- --browsers=Chrome --watch=true",
		"lint": "eslint src"
	},
	"lint-staged": {
		"*": "prettier -w",
		"*.ts": "eslint"
	},
	"dependencies": {
		"@angular/animations": "^17.3.0",
		"@angular/common": "^17.3.0",
		"@angular/compiler": "^17.3.0",
		"@angular/core": "^17.3.0",
		"@angular/forms": "^17.3.0",
		"@angular/platform-browser": "^17.3.0",
		"@angular/platform-browser-dynamic": "^17.3.0",
		"@angular/platform-server": "^17.3.0",
		"@angular/router": "^17.3.0",
		"@angular/ssr": "^17.3.10",
		"express": "^4.18.2",
		"express-rate-limit": "^5.5.1",
		"rxjs": "~7.8.0",
		"tslib": "^2.7.0",
		"zone.js": "~0.14.3"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "^17.3.10",
		"@angular-eslint/builder": "17.5.2",
		"@angular-eslint/eslint-plugin": "17.5.2",
		"@angular-eslint/eslint-plugin-template": "17.5.2",
		"@angular-eslint/schematics": "17.5.2",
		"@angular-eslint/template-parser": "17.5.2",
		"@angular/cli": "^17.3.10",
		"@angular/compiler-cli": "^17.3.0",
		"@eslint/js": "^9.12.0",
		"@tailwindcss/container-queries": "^0.1.1",
		"@types/express": "^4.17.17",
		"@types/jasmine": "~5.1.0",
		"@types/node": "^22.7.5",
		"@typescript-eslint/eslint-plugin": "^8.8.1",
		"@typescript-eslint/parser": "^8.8.1",
		"eslint": "^8.57.1",
		"globals": "^15.9.0",
		"husky": "^9.1.5",
		"jasmine-core": "^5.3.0",
		"karma": "~6.4.0",
		"karma-chrome-launcher": "~3.2.0",
		"karma-coverage": "~2.2.0",
		"karma-jasmine": "^4.0.2",
		"karma-jasmine-html-reporter": "^1.7.0",
		"lint-staged": "^15.2.9",
		"prettier": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.8",
		"tailwindcss": "^3.4.10",
		"typescript": "~5.4.2",
		"typescript-eslint": "^8.2.0"
	}
}
