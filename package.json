{
    "name": "app",
    "version": "2.2.2-0",
    "scripts": {
        "ng": "ng",
        "start": "npm run generate:api && node server.js",
        "start:production": "npm run generate:api && ng serve --configuration production --port 4200",
        "start:external-mock": "npm run generate:api && ng serve --configuration external-mock --port 4200",
        "start:full-mock": "npm run generate:api && ng serve --configuration full-mock --port 4200",
        "build": "ng build",
        "heroku-postbuild": "npm run generate:api && ng build",
        "watch": "ng build --watch --configuration development",
        "test": "ng test",
        "bundle": "rimraf dist && ng build",
        "pack": "npm pack",
        "generate:api": "npm run generate:features && npm run generate:information && npm run generate:members && npm run generate:tanks && npm run generate:wgn && npm run generate:clans && npm run generate:stronghold",
        "generate:features": "openapi-generator-cli generate -i ./open-api/features.yml -g typescript-angular -o src/generated-api/features",
        "generate:information": "openapi-generator-cli generate -i ./open-api/information.yml -g typescript-angular -o src/generated-api/information",
        "generate:members": "openapi-generator-cli generate -i ./open-api/members.yml -g typescript-angular -o src/generated-api/members",
        "generate:tanks": "openapi-generator-cli generate -i ./open-api/tanks.yml -g typescript-angular -o src/generated-api/tanks",
        "generate:wgn": "openapi-generator-cli generate -i open-api/wgn.yml -g typescript-angular -o src/generated-api/wgn",
        "generate:clans": "openapi-generator-cli generate -i open-api/clans.yml -g typescript-angular -o src/generated-api/clans",
        "generate:stronghold": "openapi-generator-cli generate -i open-api/stronghold.yml -g typescript-angular -o src/generated-api/stronghold"
    },
    "engines": {
        "node": "18.16.0",
        "npm": "9.6.7"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "17.0.8",
        "@angular/common": "17.0.8",
        "@angular/compiler": "17.0.8",
        "@angular/core": "17.0.8",
        "@angular/forms": "17.0.8",
        "@angular/material": "17.1.2",
        "@angular/platform-browser": "17.0.8",
        "@angular/platform-browser-dynamic": "17.0.8",
        "@angular/router": "17.0.8",
        "@elie29/store": "2.0.0",
        "@tailwindcss/container-queries": "0.1.1",
        "@openapitools/openapi-generator-cli": "^2.9.0",
        "express": "4.18.2",
        "express-rate-limit": "7.1.5",
        "moment": "^2.30.1",
        "ngx-cookie-service": "17.0.1",
        "ngx-skeleton-loader": "9.0.0",
        "path": "0.12.7",
        "rxjs": "7.8.1",
        "tslib": "2.6.2",
        "zone.js": "0.14.4"
    },
    "devDependencies": {
        "@angular/cli": "17.1.2",
        "@angular-devkit/build-angular": "^17.1.2",
        "@angular/compiler-cli": "17.0.8",
        "@types/jasmine": "5.1.4",
        "autoprefixer": "10.4.17",
        "@types/node": "20.11.16",
        "jasmine-core": "5.1.1",
        "karma": "6.4.2",
        "karma-chrome-launcher": "3.2.0",
        "karma-coverage": "2.2.1",
        "karma-jasmine": "5.1.0",
        "karma-jasmine-html-reporter": "2.1.0",
        "prettier-plugin-tailwindcss": "0.5.11",
        "prettier": "3.2.4",
        "tailwindcss": "3.4.1",
        "postcss": "8.4.33",
        "typescript": "5.2.2"
    }
}
